import kotlin.time.Instant;

CREATE TABLE IF NOT EXISTS details_metadata (
  entity_id TEXT PRIMARY KEY NOT NULL,
  last_updated INTEGER AS Instant NOT NULL
);

-- 1745666336 -- select unixepoch();
