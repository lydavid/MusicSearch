import kotlin.Boolean;

CREATE TABLE IF NOT EXISTS mb_event (
  id TEXT PRIMARY KEY NOT NULL,
  name TEXT NOT NULL,
  disambiguation TEXT,
  type TEXT,
  type_id TEXT,
  time TEXT,
  cancelled INTEGER AS Boolean,
  begin TEXT,
  end TEXT,
  ended INTEGER AS Boolean
);

insert:
INSERT OR REPLACE INTO mb_event
VALUES ?;

getEvent:
SELECT * FROM mb_event WHERE id = ?;
