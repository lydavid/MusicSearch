import kotlin.Boolean;

CREATE TABLE IF NOT EXISTS event (
  id TEXT PRIMARY KEY NOT NULL,
  name TEXT NOT NULL,
  disambiguation TEXT,
  type TEXT,
  type_id TEXT,
  time TEXT,
  cancelled INTEGER AS Boolean
);

insert:
INSERT INTO event(id, name, disambiguation, type, type_id, time, cancelled)
VALUES (?, ?, ?, ?, ?, ?, ?);

getEvent:
SELECT * FROM event WHERE id = ?;
