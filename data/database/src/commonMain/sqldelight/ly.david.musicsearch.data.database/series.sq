CREATE TABLE IF NOT EXISTS series (
  `id` TEXT PRIMARY KEY NOT NULL,
  `name` TEXT NOT NULL,
  `disambiguation` TEXT,
  `type` TEXT,
  `type_id` TEXT
);

insert:
INSERT OR IGNORE INTO series
VALUES ?;

getSeries:
SELECT
  id,
  name,
  disambiguation,
  type
FROM series WHERE id = ?;

delete:
DELETE FROM series
WHERE id = :id;
