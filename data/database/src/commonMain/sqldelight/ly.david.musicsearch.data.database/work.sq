import kotlin.String;
import kotlin.collections.List;

CREATE TABLE IF NOT EXISTS work (
  id TEXT PRIMARY KEY NOT NULL,
  name TEXT NOT NULL,
  disambiguation TEXT,
  type TEXT,
  type_id TEXT,
  language TEXT,
  iswcs TEXT AS List<String>
);

insert:
INSERT OR IGNORE INTO work
VALUES ?;

getWork:
SELECT
  id,
  name,
  disambiguation,
  type,
  language,
  iswcs
FROM work WHERE id = ?;

delete:
DELETE FROM work
WHERE id = :id;
